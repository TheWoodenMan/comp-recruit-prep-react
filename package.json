    "axios": "^1.3.6",
    "connect-mongo": "^5.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "framer-motion": "^10.12.4",
    "mongodb": "^5.9.2",
    "mongoose": "^7.1.0",
    "morgan": "^1.10.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.8.0",
    "react-router-dom": "^6.10.0"
  },
  "devDependencies": {
    "@storybook/addon-a11y": "^7.0.9",
